package de.hsh.zahlenarraytest.Zeitmesser;

public class Main {

    public static void main(String[] args) {
        Zeitmesser m = new Zeitmesser();
        m.start();
        for (int i = 0; i < 1000000000; i++) {
            i+=i;
        }
        m.stop();
        System.out.println(m.getGemesseneGesamtzeit());
    }

}
